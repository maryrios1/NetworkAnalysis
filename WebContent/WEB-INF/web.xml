<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>NetworkAnalysis</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>NetworkAnalysis</servlet-name>
    <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>com.sun.jersey.config.property.packages</param-name>
      <param-value>com.NetworkAnalysis.app</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>NetworkAnalysis</servlet-name>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ExportToExcel</servlet-name>
    <servlet-class>com.NetworkAnalysis.srt.ExportToExcelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExportToExcel</servlet-name>
    <url-pattern>/ExportToExcel</url-pattern>
  </servlet-mapping>
   <!-- 
   <servlet>
    <servlet-name>RestartStopSearchServlet</servlet-name>
    <servlet-class>com.NetworkAnalysis.srt.RestartStopSearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RestartStopSearchServlet</servlet-name>
    <url-pattern>/RestartStopSearchServlet</url-pattern>
  </servlet-mapping> -->
</web-app>